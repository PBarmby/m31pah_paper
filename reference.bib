@article{Mould:1989lr,
	Abstract = {In 0.5 arcsec seeing, the 2.2 micron light distribution in the nucleus of M31 resembles the optical light distribution. The asymmetry seen in the optical data, a positive skew on the SW major axis, is also seen in the IR. It follows that the asymmetry is not an artifact of selective foreground extinction by dust. Either the asymmetry exists in the distribution of stars, or it is produced by a dust disk which is optically thick at 2.2 microns and lies within the nucleus, perpendicular to the outer disk of M31. An upper limit of 0.3 mag is put on any V - K excess in the central arcsecond of M31. This is sufficient to exclude a cluster of low-mass stars as the source of the high mass-to-light ratio in the nucleus, for any stellar mass exceeding the main-sequence hydrogen burning limit.},
	Adsnote = {Provided by the SAO/NASA Astrophysics Data System},
	Adsurl = {http://adsabs.harvard.edu/abs/1989ApJ...339L..21M},
	Author = {{Mould}, J. and {Graham}, J. and {Matthews}, K. and {Soifer}, B.~T. and {Phinney}, E.~S.},
	Date-Added = {2014-02-20 18:52:28 -0500},
	Date-Modified = {2014-02-20 18:52:28 -0500},
	Doi = {10.1086/185410},
	Journal = {\apjl},
	Keywords = {Andromeda Galaxy, Galactic Nuclei, Infrared Imagery, Isophotes, Radial Distribution},
	Month = apr,
	Pages = {L21-L24},
	Title = {{Infrared images of the nucleus of M31}},
