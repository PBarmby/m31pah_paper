	Doi = {10.1088/0004-637X/693/2/L136},
	Eprint = {0902.1187},
	Journal = {\apjl},
	Keywords = {galaxies: individual: NGC 2110, galaxies: Seyfert, infrared: galaxies},
	Month = mar,
	Pages = {L136-L140},
	Primaryclass = {astro-ph.GA},
	Title = {{The Origin of the Silicate Emission Features in the Seyfert 2 Galaxy NGC 2110}},
