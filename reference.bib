	Eprint = {1205.0029},
	Journal = {\aj},
	Keywords = {galaxies: active, galaxies: nuclei, galaxies: photometry, galaxies: Seyfert, infrared: galaxies},
	Month = jul,
	Pages = {11},
	Primaryclass = {astro-ph.CO},
	Title = {{The Nuclear Infrared Emission of Low-luminosity Active Galactic Nuclei}},
	Volume = 144,
	Year = 2012,
